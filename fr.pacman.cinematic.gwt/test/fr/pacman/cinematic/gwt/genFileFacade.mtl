[comment @main/]
[template public genFlowManager_facade(root : cinematic::CinematicRoot)]
  [for (flow : flow::Flow | root.eAllContents(flow::Flow))]
    [flow.genFlowManager()/]
  [/for]
[/template]

[comment @main/]
[template public genGwtLanceur_facade(root : cinematic::CinematicRoot)]
