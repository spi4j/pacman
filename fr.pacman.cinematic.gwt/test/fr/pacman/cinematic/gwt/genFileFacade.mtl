[comment encoding = UTF-8 /]
[module genFileFacade('http://www.obeonetwork.org/dsl/cinematic/3.0.0')]

[import fr::pacman::cinematic::gwt::files::genAppliAsyncCallback /]
[import fr::pacman::cinematic::gwt::files::genFlowManager /]
[import fr::pacman::cinematic::gwt::files::genGwtLanceur /]
[import fr::pacman::cinematic::gwt::files::genGwtViews /]
[import fr::pacman::cinematic::gwt::files::genModuleGwtXml /]
[import fr::pacman::cinematic::gwt::files::genPresenter /]
[import fr::pacman::cinematic::gwt::files::genViewImplementation /]

[template public genAppliAsyncCallback_facade(root : CinematicRoot)]
[comment @main/]
[genAppliAsyncCallback(root)/]
[/template]

[template public genFlowManager_facade(root : CinematicRoot)]
[comment @main/]
[for (flow : Flow | root.eAllContents(Flow))]
[genFlowManager(flow)/]
[/for]
[/template]

[template public genGwtLanceur_facade(root : CinematicRoot)]
[comment @main/]
[genGwtLanceur(root)/]
[/template]

[template public genGwtViews_facade(root : CinematicRoot)]
[comment @main/]
[genGwtViews(root, root.eAllContents(ViewContainer)->sortedBy(name))/]
[/template]

[template public genModuleGwtXml_facade(root : CinematicRoot)]
[comment @main/]
[genModuleGwtXml(root)/]
[/template]

[template public genPresenter_facade(root : CinematicRoot)]
[comment @main/]
[for (viewContainer : ViewContainer | root.eAllContents(ViewContainer))]
[genPresenter(viewContainer)/]
[/for]
[/template]

[template public genViewImplementation_facade(root : CinematicRoot)]
[comment @main/]
[for (viewContainer : ViewContainer | root.eAllContents(ViewContainer))]
[genViewImplementation(viewContainer)/]
[/for]
[/template]
