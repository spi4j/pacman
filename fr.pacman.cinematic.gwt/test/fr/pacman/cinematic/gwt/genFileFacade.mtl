[comment @main/]
[template public genGwtViews_facade(root : cinematic::CinematicRoot)]
  [root.genGwtViews(root.eAllContents(view::ViewContainer)->sortedBy(temp1 | temp1.name))/][/template]

[comment @main/]
[template public genModuleGwtXml_facade(root : cinematic::CinematicRoot)]
  [root.genModuleGwtXml()/][/template]

[comment @main/]
[template public genPresenter_facade(root : cinematic::CinematicRoot)]
  [for (viewContainer : view::ViewContainer | root.eAllContents(view::ViewContainer))]
    [viewContainer.genPresenter()/]
  [/for]
[/template]

[comment @main/]
[template public genViewImplementation_facade(root : cinematic::CinematicRoot)]
