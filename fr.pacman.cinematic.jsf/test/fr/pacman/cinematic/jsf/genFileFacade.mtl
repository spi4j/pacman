[module genFileFacade('http://www.obeonetwork.org/dsl/cinematic/3.0.0', 'http://www.obeonetwork.org/dsl/cinematic/view/1.0.0', 'http://www.obeonetwork.org/dsl/cinematic/flow/1.0.0', 'http://www.obeonetwork.org/dsl/cinematic/toolkits/1.0.0')/]

[import fr::pacman::cinematic::jsf::files::genBean/]
[import fr::pacman::cinematic::jsf::files::genViewImplementation/]

[comment encoding = UTF-8 /]
[comment @main/]
[template public genBean_facade(root : cinematic::CinematicRoot)]
  [for (aViewState : flow::ViewState | root.eAllContents(flow::ViewState))]
    [for (viewContainer : view::ViewContainer | aViewState.viewContainers)]
      		[viewContainer.genBean(aViewState)/]
    [/for]
  [/for]
[/template]

[comment @main/]
[template public genViewImplementation_facade(root : cinematic::CinematicRoot)]
  [for (aViewState : flow::ViewState | root.eAllContents(flow::ViewState))]
    [for (viewContainer : view::ViewContainer | aViewState.viewContainers)]
      		[viewContainer.genViewImplementation(aViewState)/]
    [/for]
  [/for]
[/template]