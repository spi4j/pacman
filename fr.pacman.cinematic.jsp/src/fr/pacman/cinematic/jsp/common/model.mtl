[module model('http://www.obeonetwork.org/dsl/environment/3.0.0', 'http://www.obeonetwork.org/dsl/soa/4.0.0')/]

[comment encoding = UTF-8 /]
[comment : /]
[query public hasOutputDto(o : soa::Operation) : Boolean = not o.output->isEmpty() and not (o.output->first() = null) and o.output->first().type.oclIsTypeOf(environment::DTO)/]

[query public paramsIn(inputs : OrderedSet(soa::Parameter)) : Sequence(String) = inputs->asSequence()->collect(temp1 | temp1.paramIn())/]

[query private paramIn(param : soa::Parameter) : String = 'null'/]