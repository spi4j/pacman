[module genTypePrimitif('http://www.obeonetwork.org/dsl/environment/3.0.0')/]

[import fr::pacman::commons::common::packages/]
[import fr::pacman::commons::types::packages/]
[import fr::pacman::commons::types::typePrimitif/]

[comment encoding = UTF-8 /]
[template public genTypePrimitif(t : environment::PrimitiveType)]
  [file (t.getCommonsSourceDir().filename(t.buildPackageTypePrimitif(), t.name.toUpperFirst() + '.java'), overwrite, 'UTF-8')]
    [t.typePrimitif()/]
  [/file]
[/template]