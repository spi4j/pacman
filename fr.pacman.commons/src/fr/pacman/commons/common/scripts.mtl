[module scripts('http://www.eclipse.org/ocl/1.1.0/Ecore')/]

[import fr::pacman::commons::common::properties/]
[import fr::pacman::commons::common::names/]

[comment encoding = UTF-8 /]
[query public buildEmbeddedHttpScriptName(any : ecore::EObject) : String = 'Embedded' + any.getHttpEmbeddedServer().toUpperFirst() + '.bat'/]

[query public buildEmbeddedHttpStartupScriptName(any : ecore::EObject) : String = any.getApplicationNameUpperFirst() + 'Startup.bat'/]