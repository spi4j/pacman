[module genEjbJar('http://www.eclipse.org/ocl/1.1.0/Ecore')/]

[import fr::pacman::start::project::EjbJar/]
[import fr::pacman::commons::common::names/]
[import fr::pacman::commons::common::packages/]

[comment encoding = UTF-8 /]
[template public genEjbJar(any : ecore::EObject)]
  [file (any.getClientJsfProjectName().filename(any.getClientJsfWebDir(), 'ejb-jar.xml'), overwrite, 'UTF-8')]
    [any.EjbJar()/]
  [/file]
[/template]