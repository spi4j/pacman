[comment : Write all required imports. /]
[template private writeImports(m : soa::System) post(self.trim())]
  [m.initImports(true)/]
  [m.addImport('jakarta.servlet.ServletConfig')/]
  
  [for (service : soa::Service | m.eAllContents(soa::Service))]
    	[m.addImport(service.buildPackageImplWs() + '.' + service.buildServiceFacadeCXFClassName(), service.hasCXFOperations())/]
  [/for]
  
  [m.flushImports()/][/template]